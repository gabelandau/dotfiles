# Shortcuts
alias c='clear'

# Git
alias gs='git status'
alias gd='git diff'
alias gaa='git add .'
alias gpo='git push origin'
alias gpom='git push origin master'
alias gcmsg='git commit -m'

# Directories
alias zshd='cd ~/.zshrc.d'

# Config
alias aliases='code ~/.zshrc.d/500.aliases.zshrc'
alias areload='source ~/.zshrc.d/500.aliases.zshrc'
alias dotfiles='cd ~/Development/dotfiles'

# System
alias python='python3'
